{
  "name": "FitNote",
  "description": "Fit Note Front-end",
  "version": "1.7.0",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node ./bin/server.js",
    "restart": "npm run build && npm run start",
    "dev": "NODE_ENV=test node ./bin/server.js | bunyan -o short",
    "prod": "NODE_ENV=prod node ./bin/server.js",
    "build": "./node_modules/.bin/gulp jenkins && npm run build:js  && npm run build:html5shiv && npm run build:scss && npm run build:scss-ie8",
    "build:html5shiv": "cp ./node_modules/html5shiv/dist/html5shiv.min.js ./public/javascript",
    "build:scss": "sass assets/hmrc/all.scss -I node_modules --no-source-map | postcss -o public/stylesheets/ui-fitnote-frontend.css",
    "build:scss-ie8": "sass assets/hmrc/all-ie8.scss -I node_modules --no-source-map | IE8=true postcss -o public/stylesheets/ui-fitnote-frontend-ie8.css",
    "build:js": "rollup -c",
    "test": "npm run test:unit",
    "test:unit": "mocha -- -- test/unit/**/*",
    "deps:update": "npm-check-updates -u && npm i",
    "quality:coverage": "nyc node_modules/.bin/_mocha -- -- test/unit/**/*",
    "quality:lint:fix": "eslint --fix .",
    "quality:lint": "eslint .",
    "quality:stylelint": "stylelint \"assets/**/*.scss\" -f verbose",
    "security:vulnerable-packages": "nsp check",
    "testing": "NODE_ENV=test node ./bin/server.js | bunyan -o short",
    "ioredis": "5.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/eslint-parser": "7.21.3",
    "@babel/preset-env": "7.23.2",
    "@dwp/eslint-config-base": "5.0.1",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-legacy": "2.2.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "chai": "4.3.7",
    "chai-http": "4.3.0",
    "chai-spies": "1.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "gulp": "4.0.2",
    "hmrc-frontend-src": "https://github.com/hmrc/hmrc-frontend.git#v4.9.1",
    "html5shiv": "3.7.3",
    "mocha": "9.2.2",
    "nock": "13.3.0",
    "npm-check-updates": "11.8.5",
    "oldie": "1.3.0",
    "postcss-cli": "8.3.1",
    "postcss-url": "10.1.3",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.59.3",
    "sinon": "12.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-scss": "3.21.0"
  },
  "dependencies": {
    "@dwp/govuk-casa": "^7.2.2",
    "async": "3.2.6",
    "autoprefixer": "10.4.20",
    "body-parser": "1.20.3",
    "bunyan": "1.8.15",
    "bunyan-syslog-udp": "0.2.0",
    "chokidar": "4.0.1",
    "config": "3.3.12",
    "connect-timeout": "1.9.0",
    "cookie-parser": "1.4.7",
    "del": "6.1.1",
    "express": "4.21.1",
    "express-sanitized": "0.5.1",
    "express-session": "^1.18.1",
    "filesize": "10.1.6",
    "govuk-frontend": "4.5.0",
    "gulp-concat": "2.6.1",
    "gulp-csso": "4.0.1",
    "gulp-dart-sass": "1.1.0",
    "gulp-debug": "4.0.0",
    "gulp-mocha": "8.0.0",
    "gulp-postcss": "10.0.0",
    "gulp-uglify": "3.0.2",
    "gulp4-run-sequence": "1.0.2",
    "helmet": "8.0.0",
    "hmrc-frontend": "4.9.1",
    "multer": "1.4.5-lts.1",
    "notifications-node-client": "7.0.3",
    "nunjucks": "3.2.4",
    "nyc": "17.1.0",
    "postcss": "8.4.48",
    "pump": "3.0.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "require-dir": "1.2.0",
    "serve-favicon": "2.5.0",
    "uid-safe": "2.1.5",
    "underscore": "1.13.7",
    "valimate-notifier": "1.0.2"
  },
  "overrides": {
    "anymatch": {
      "braces": "3.0.3",
      "micromatch": "4.0.8"
    },
    "axios": "0.28.0",
    "chokidar": "$chokidar",
    "express": "$express",
    "findup-sync": {
      "braces": "3.0.3",
      "micromatch": "4.0.8"
    },
    "glob-parent": "^5.1.2",
    "got": "12.1.0",
    "gulp": {
      "braces": "3.0.3"
    },
    "matchdep": {
      "braces": "3.0.3",
      "micromatch": "4.0.8"
    },
    "micromatch": {
      "braces": "3.0.3"
    },
    "minimatch": "3.0.5",
    "nanoid": "3.1.31",
    "oldie": {
      "postcss": "$postcss"
    },
    "pixrem": {
      "postcss": "^7.0.36"
    },
    "postcss-less": {
      "postcss": "$postcss"
    },
    "postcss-pseudoelements": {
      "postcss": "$postcss"
    },
    "postcss-sass": {
      "postcss": "$postcss"
    },
    "postcss-scss": {
      "postcss": "^7.0.36"
    },
    "postcss-safe-parser": {
      "postcss": "$postcss"
    },
    "stylelint": {
      "postcss": "$postcss"
    },
    "tough-cookie": "4.1.3"
  }
}
